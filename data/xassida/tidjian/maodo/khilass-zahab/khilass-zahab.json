{
  "name": "khilass-zahab",
  "chapters": [
    {
      "name": "الفاتحة",
      "number": 1,
      "verses": [
        {
          "number": 1,
          "key": "1:1",
          "text": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
          "words": [
            {
              "position": 0,
              "text": "بِسْمِ",
              "transcription": "bismi"
            },
            {
              "position": 1,
              "text": "اللَّهِ",
              "transcription": "l-lahi"
            },
            {
              "position": 2,
              "text": "الرَّحْمَٰنِ",
              "transcription": "r-raḥmāni"
            },
            {
              "position": 3,
              "text": "الرَّحِيمِ",
              "transcription": "r-raḥīmi"
            }
          ],
          "translations": [
            {
              "lang": "en",
              "text": "In the name of Allah, the Entirely Merciful, the Especially Merciful.",
              "author": ""
            },
            {
              "lang": "fr",
              "text": "Au nom d'Allah, le Tout Miséricordieux, le Très Miséricordieux.",
              "author": ""
            }
          ]
        },
        {
          "number": 2,
          "key": "1:2",
          "text": "الْحَمْدُ لِلَّهِ رَبِّ الْعَالَمِينَ",
          "words": [
            {
              "position": 0,
              "text": "الْحَمْدُ",
              "transcription": "al-ḥamdu"
            },
            {
              "position": 1,
              "text": "لِلَّهِ",
              "transcription": "lillahi"
            },
            {
              "position": 2,
              "text": "رَبِّ",
              "transcription": "rabbi"
            },
            {
              "position": 3,
              "text": "الْعَالَمِينَ",
              "transcription": "l-ʿālamīna"
            }
          ],
          "translations": [
            {
              "lang": "en",
              "text": "[All] praise is [due] to Allah, Lord of the worlds -",
              "author": ""
            },
            {
              "lang": "fr",
              "text": "Louange à Allah, Seigneur de l'univers.",
              "author": ""
            }
          ]
        },
        {
          "number": 3,
          "key": "1:3",
          "text": "الرَّحْمَٰنِ الرَّحِيمِ",
          "words": [
            {
              "position": 0,
              "text": "الرَّحْمَٰنِ",
              "transcription": "ar-raḥmāni"
            },
            {
              "position": 1,
              "text": "الرَّحِيمِ",
              "transcription": "r-raḥīmi"
            }
          ],
          "translations": [
            {
              "lang": "en",
              "text": "The Entirely Merciful, the Especially Merciful,",
              "author": ""
            },
            {
              "lang": "fr",
              "text": "Le Tout Miséricordieux, le Très Miséricordieux,",
              "author": ""
            }
          ]
        },
        {
          "number": 4,
          "key": "1:4",
          "text": "مَالِكِ يَوْمِ الدِّينِ",
          "words": [
            {
              "position": 0,
              "text": "مَالِكِ",
              "transcription": "māliki"
            },
            {
              "position": 1,
              "text": "يَوْمِ",
              "transcription": "īawmi"
            },
            {
              "position": 2,
              "text": "الدِّينِ",
              "transcription": "d-dīni"
            }
          ],
          "translations": [
            {
              "lang": "en",
              "text": "Sovereign of the Day of Recompense.",
              "author": ""
            },
            {
              "lang": "fr",
              "text": "Maître du Jour de la rétribution.",
              "author": ""
            }
          ]
        },
        {
          "number": 5,
          "key": "1:5",
          "text": "إِيَّاكَ نَعْبُدُ وَإِيَّاكَ نَسْتَعِينُ",
          "words": [
            {
              "position": 0,
              "text": "إِيَّاكَ",
              "transcription": "'iīāka"
            },
            {
              "position": 1,
              "text": "نَعْبُدُ",
              "transcription": "naʿbudu"
            },
            {
              "position": 2,
              "text": "وَإِيَّاكَ",
              "transcription": "wa'iīāka"
            },
            {
              "position": 3,
              "text": "نَسْتَعِينُ",
              "transcription": "nastaʿīnu"
            }
          ],
          "translations": [
            {
              "lang": "en",
              "text": "It is You we worship and You we ask for help.",
              "author": ""
            },
            {
              "lang": "fr",
              "text": "C'est Toi [Seul] que nous adorons, et c'est Toi [Seul] dont nous implorons secours.",
              "author": ""
            }
          ]
        },
        {
          "number": 6,
          "key": "1:6",
          "text": "اهْدِنَا الصِّرَاطَ الْمُسْتَقِيمَ",
          "words": [
            {
              "position": 0,
              "text": "اهْدِنَا",
              "transcription": "ihdinā"
            },
            {
              "position": 1,
              "text": "الصِّرَاطَ",
              "transcription": "ṣ-ṣirāṭa"
            },
            {
              "position": 2,
              "text": "الْمُسْتَقِيمَ",
              "transcription": "l-mustaqīma"
            }
          ],
          "translations": [
            {
              "lang": "en",
              "text": "Guide us to the straight path -",
              "author": ""
            },
            {
              "lang": "fr",
              "text": "Guide-nous dans le droit chemin,",
              "author": ""
            }
          ]
        },
        {
          "number": 7,
          "key": "1:7",
          "text": "صِرَاطَ الَّذِينَ أَنْعَمْتَ عَلَيْهِمْ غَيْرِ الْمَغْضُوبِ عَلَيْهِمْ وَلَا الضَّالِّينَ",
          "words": [
            {
              "position": 0,
              "text": "صِرَاطَ",
              "transcription": "ṣirāṭa"
            },
            {
              "position": 1,
              "text": "الَّذِينَ",
              "transcription": "l-aḏīna"
            },
            {
              "position": 2,
              "text": "أَنْعَمْتَ",
              "transcription": "'aanʿamta"
            },
            {
              "position": 3,
              "text": "عَلَيْهِمْ",
              "transcription": "ʿalaīhim"
            },
            {
              "position": 4,
              "text": "غَيْرِ",
              "transcription": "ġaīri"
            },
            {
              "position": 5,
              "text": "الْمَغْضُوبِ",
              "transcription": "l-maġḍwbi"
            },
            {
              "position": 6,
              "text": "عَلَيْهِمْ",
              "transcription": "ʿalaīhim"
            },
            {
              "position": 7,
              "text": "وَلَا",
              "transcription": "walā"
            },
            {
              "position": 8,
              "text": "الضَّالِّينَ",
              "transcription": "ḍ-ḍāllīna"
            }
          ],
          "translations": [
            {
              "lang": "en",
              "text": "The path of those upon whom You have bestowed favor, not of those who have evoked [Your] anger or of those who are astray.",
              "author": ""
            },
            {
              "lang": "fr",
              "text": "le chemin de ceux que Tu as comblés de faveurs, non pas de ceux qui ont encouru Ta colère, ni des égarés.",
              "author": ""
            }
          ]
        }
      ],
      "translated_names": [
        {
          "lang": "en",
          "translation": "Al-Fatiha",
          "transcription": ""
        },
        {
          "lang": "fr",
          "translation": "Al-Fatiha",
          "transcription": ""
        }
      ]
    }
  ],
